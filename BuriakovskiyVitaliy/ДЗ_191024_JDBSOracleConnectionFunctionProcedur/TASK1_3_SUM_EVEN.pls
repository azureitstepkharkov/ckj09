create or replace FUNCTION    TASK1_3_SUM_EVEN(
    FIRST_NAMBER IN NUMBER DEFAULT 0 ,
    LAST_NAMBER  IN NUMBER DEFAULT 100 )
  RETURN NUMBER
IS
  i        INTEGER :=FIRST_NAMBER;
  SUM_EVEN INTEGER :=0;
BEGIN
  WHILE (I<=LAST_NAMBER)
  LOOP
    IF (MOD(I,2)=0) THEN
      SUM_EVEN :=SUM_EVEN +I;
    END IF;
    I :=I +1;
  END LOOP;
RETURN SUM_EVEN;
END TASK1_3_SUM_EVEN;